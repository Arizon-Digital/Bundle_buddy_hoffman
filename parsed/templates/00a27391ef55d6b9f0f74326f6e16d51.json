{
  "components/products/wishlist-custom": "<div action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action{{#if product.can_purchase}} has-add-cart{{/if}}\" data-wishlist-add method=\"post\">\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown\">\n        {{#if customer.wishlists}}\n            {{#each customer.wishlists}}\n                <li>\n                    <span product-id=\"{{../product.id}}\" class=\"wishlist-custom button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\"\n                    href=\"{{this.add_url}}&product_id={{../product.id}}\">{{this.name}}</span>\n                </li>\n            {{/each}}\n        {{else}}\n        <li>\n            <a href=\"\">{{lang 'account.wishlists.add_to_default'}}</a>\n        </li>\n        {{/if}}\n        <li>\n            <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\n        </li>\n    </ul>\n</div>\n"
}
